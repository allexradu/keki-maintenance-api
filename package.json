        "drizzle-orm": "^0.35.3",
        "drizzle-zod": "^0.5.1",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-ft-flow": "^3.0.11",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.8.3",
        "eslint-plugin-prettier": "^5.2.1",
        "hono": "^4.6.6",
